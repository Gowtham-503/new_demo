{
  "name": "new_demo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.js",

  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "electron": "electron .",
    "dist": "set CSC_IDENTITY_AUTO_DISCOVERY=false && npm run build && electron-builder",
    "lint": "eslint .",
    "preview": "vite preview"
  },

  "build": {
    "appId": "com.example.newdemo",
    "productName": "NewDemo",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "package.json"
    ],
    "forceCodeSigning": false,
    "win": {
      "target": "nsis"
      
    }
  },

  "dependencies": {
    "@reduxjs/toolkit": "^2.9.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0"
  },

  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "electron": "^39.2.7",
    "electron-builder": "^26.0.12",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "vite": "^7.1.7"
  }
}
